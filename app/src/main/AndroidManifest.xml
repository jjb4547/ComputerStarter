<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.computerstarter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="ComputerStarter"
        android:roundIcon="@mipmap/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComputerStarter">
        <activity
            android:name=".PC_Part_Activity"
            android:exported="false"
            android:label="@string/title_activity_pc_part" />
        <activity
            android:name=".Education_Tabbed"
            android:exported="false"
            android:label="@string/title_activity_education_tabbed" />
        <activity
            android:name=".EducationActivities"
            android:exported="true" />
        <activity
            android:name=".real_profile"
            android:exported="true"
            android:label="@string/title_activity_real_profile" />
        <activity
            android:name=".real_register"
            android:exported="true"
            android:label="@string/title_activity_real_register" />
        <activity
            android:name=".real_login"
            android:exported="true"
            android:label="@string/title_activity_real_login" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Results_Activity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".SocialMediaActivity" />
        <activity android:name=".Build_Activity" />
        <activity android:name=".PC_Build_Parts"/>
        <activity android:name=".MyBuildActivity"/>
        <activity android:name=".SocialMediaBlogs"/>
        <activity android:name=".Login_SignUpActivity"
            android:theme="@style/AppTheme.NoActionBar"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>